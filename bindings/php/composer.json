{
    "name": "strling/strling",
    "description": "STRling - A next-generation production-grade syntax for writing powerful regular expressions with an object-oriented approach and instructional error handling",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "TheCyberLocal",
            "email": "thecyberlocal@proton.me"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "STRling\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "STRling\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
